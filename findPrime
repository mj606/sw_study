import java.util.*;

public class Solution {
	HashSet<Integer> hs = new HashSet<>();

	public boolean isPrime(int n) {
		// 0과 1은 소수가 아님.
		if (n == 0 || n == 1)
			return false;
		
		// n보다 작은 수로 나누어 떨어지면 소수가 아님.
		for(int i = 2; i < n; i++) {
			if(n % i == 0)
				return false;
		}
		
		return true;
	}

	public void recursive(String s, String others) {
		// 현재 문자열을 HashSet에 추가함.
		if (!s.equals(""))
			hs.add(Integer.valueOf(s));

		// 숫자를 하나 더해 새로운 조합을 만듦.
		// subsitrng로 남은 문자열에서 i를 잘라냄.
		for (int i = 0; i < others.length(); i++)
			recursive(s + others.charAt(i), others.substring(0, i) + others.substring(i + 1));
	}

	public int solution(String numbers) {
		// 모든 숫자의 조합을 만듦.
		recursive("", numbers);

		int count = 0;	// 소수의 개수.
		Iterator<Integer> it = hs.iterator();
		while (it.hasNext()) {
			int number = it.next();	// hs에 있는 숫자의 조합.
			if (isPrime(number))
				count++;
		}

		return count;
	}
}
